<?xml version = "1.0" encoding = "UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd ">

  <!-- Initialization for post config -->

  <import resource="classpath:sjph/life/data/database/config/life-data-database-dao-config.xml" />

  <bean id="sjph.life.web.post.PostHandler" class="sjph.life.web.post.PostHandler">
    <property name="postDao" ref="sjph.life.data.database.dao.PostDao" />
  </bean>

  <bean id="sjph.life.web.post.PostController" class="sjph.life.web.post.PostController">
    <property name="postHandler" ref="sjph.life.web.post.PostHandler" />
  </bean>

</beans>